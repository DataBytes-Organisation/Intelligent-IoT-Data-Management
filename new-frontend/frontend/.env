# Your backend base URL
VITE_API_BASE_URL=http://localhost:8000


# Choose one:
# - "jwt": backend returns { token, user }
# - "cookie": backend sets an httpOnly cookie and returns { user }
VITE_AUTH_MODE=jwt

# Optional: endpoints (change if yours differ)
VITE_LOGIN_PATH=/api/auth/login
VITE_LOGOUT_PATH=/api/auth/logout
VITE_ME_PATH=/api/auth/me
VITE_REGISTER_PATH=/api/auth/register
VITE_REFRESH_PATH=/api/auth/refresh